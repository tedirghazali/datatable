{
  "name": "tedir-table",
  "version": "0.0.1-alpha-4",
  "description": "Tedir Table is a Vue 3 & Web Component DataTable",
  "main": "dist/vue/tedir-table-vue.umd.js",
  "module": "dist/vue/tedir-table-vue.es.js",
  "unpkg": "dist/vue/tedir-table-vue.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vue/tedir-table-vue.es.js",
      "require": "./dist/vue/tedir-table-vue.umd.js"
    },
    "./web": {
      "import": "./dist/web/tedir-table-web.es.js",
      "require": "./dist/web/tedir-table-web.umd.js"
    },
    "./css": "./dist/vue/style.css"
  },
  "scripts": {
    "dev-vue": "vite --config vite-vue-config.ts",
    "build-vue": "vue-tsc --noEmit && vite build --config vite-vue-config.ts",
    "build-web": "vue-tsc --noEmit && vite build --config vite-web-config.ts",
    "preview-vue": "vite preview --config vite-vue-config.ts"
  },
  "files": [
    "src/alga/*.alga",
    "package.alga",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedirghazali/datatable.git"
  },
  "keywords": [
    "datatable",
    "vue3table",
    "usetable"
  ],
  "author": "Tedir Ghazali",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/tedirghazali/datatable/issues"
  },
  "homepage": "https://github.com/tedirghazali/datatable#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "alga-css": "^1.0.0-emerald-2",
    "alga-js": "^0.1.0-iron-1",
    "alga-vue": "^0.0.1-stone-4",
    "typescript": "^4.6.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.4"
  },
  "dependencies": {
    "vue": "^3.2.37"
  }
}
